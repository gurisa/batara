  },
  "dependencies": {
    "express": "^4.17.1",
    "express-locale": "^2.0.0",
    "express-validator": "^6.10.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "node-polyglot": "^2.4.0",
    "rotating-file-stream": "^2.1.5"
  },
